# Summarize data for pie chart
pie_data <- fuel_eco %>%
filter(fuel_type %in% c("Diesel", "Regular", "Premium"),
highway_mpg_ft1 <= 50) %>%
count(fuel_type) %>%
mutate(prop = n / sum(n) * 100,
ypos = cumsum(prop) - 0.5 * prop)
# Create pie chart
ggplot(pie_data, aes(x = "", y = prop, fill = fuel_type)) +
geom_bar(stat = "identity", width = 1, color = "white") +
coord_polar("y", start = 0) +
theme_void() +
geom_text(aes(y = ypos, label = paste0(fuel_type, "\n", round(prop, 1), "%")), color = "white", size = 5) +
labs(title = "Proportion of Fuel Types (≤50 MPG)")
